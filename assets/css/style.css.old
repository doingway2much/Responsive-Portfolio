}
li {
    font-family: Arial, Helvetica, sans-serif
}
p {
    font-family: Arial, Helvetica, sans-serif
}

a {
    color: #00183C;
    text-decoration: none;
}

input[type=text], select, textarea {
    width: 100%;
    padding: 12px;
    border: 1px solid #ccc;
    border-radius: 4px;
    box-sizing: border-box;
    margin-top: 6px;
    margin-bottom: 16px;
    resize: vertical;
}

input[type=submit] {
    background-color: #00183C;
    color: white;
}
a:hover {
    color: mediumblue;
}

img {
    float: left;
    padding: 1px;
    margin: 10px;
    border-radius: 5px;
    border-style:  groove;
    border-color: #00183C;
}

.header {
    border-bottom-style: groove;
    border-bottom-width: .2em;
    border-color: #03BCF2;
}

#name {   
    margin-left: 15%; 
    margin-right: 25%;
    padding-left: 25px;
    padding-right: 25px;
    background: #00183C;
    color:white;
    float: left;
}

#links {
   font-family: Arial, Helvetica, sans-serif;
   font-weight: bold;
   margin-right: 100px;
   /* margin-right: 25%; */
   background: white;
   padding: 25px;
   /* text-align: right; */
   word-spacing: 1em;
}

.container {
    clear: left;
    background-image: url('https://marketingweek.imgix.net/content/uploads/2017/05/12103940/Coding-featured.jpg');
    background-repeat: no-repeat;
    min-height: 90vh;
    margin: 0;
    background-size: cover;
}

#main-text {
    border-radius: 5px;
    /* padding: 20px; */
    background: white;   
    border-style: outset;
    width: 960px;
    height: 600px;
    text-overflow: unset; 
    border-style:  groove;
    border-color: #00183C;
}
